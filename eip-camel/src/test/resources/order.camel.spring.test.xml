<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="properties"
		class="org.apache.camel.component.properties.PropertiesComponent">
		<property name="location" value="classpath:eip.camel.test.properties" />
	</bean>

	<bean id="orderService" factory-method="mock" class="org.mockito.Mockito">
		<constructor-arg value="eip.common.services.OrderService"/>
	</bean>
	
	<bean id="backlogService" factory-method="mock" class="org.mockito.Mockito">
		<constructor-arg value="eip.common.services.BacklogService"/>
	</bean>

</beans>
